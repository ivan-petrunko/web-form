{
    "name": "ivan-petrunko/web-form",
    "description": "PHP web form.",
    "keywords": ["php", "web", "form", "builder"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Petrunko",
            "email": "ivan@petrunko.com",
            "homepage": "https://petrunko.com/"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Petrunko\\Web\\Form\\": "src/Petrunko/Web/Form/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Petrunko\\Web\\Form\\": "tests/Petrunko/Web/Form/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
